<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="200.0" prefWidth="586.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="intersectionsColumn" editable="false" prefWidth="75.0" reorderable="false" sortable="false" text="Intersections Found" />
       <TableColumn fx:id="segmentsColumn" editable="false" prefWidth="75.0" reorderable="false" sortable="false" text="Segments Involved" />
     </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
   </TableView>
</fx:root>
