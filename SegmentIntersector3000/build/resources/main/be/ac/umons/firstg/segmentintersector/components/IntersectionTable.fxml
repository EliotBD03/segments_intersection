<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<fx:root fx:id="table" stylesheets="@../test.css" type="TableView" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
  <columns>
    <TableColumn fx:id="intersectionsColumn" editable="false" reorderable="false" sortable="false" text="Intersections Found" />
    <TableColumn fx:id="segmentsColumn" editable="false" reorderable="false" sortable="false" text="Segments Involved" />
    <TableColumn fx:id="highlightColumn" editable="false" prefWidth="20.0" reorderable="false" sortable="false" />
  </columns>
   <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
   </columnResizePolicy>
</fx:root>
